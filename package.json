{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:push": "npx prisma db push"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "typescript",
    "casl",
    "jwt",
    "starter",
    "boilerplate",
    "mongodb"
  ],
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/nestjs": "^6.0.1",
    "@adminjs/prisma": "^4.0.0",
    "@aws-sdk/client-s3": "3.332.0",
    "@casl/ability": "6.5.0",
    "@nestjs/common": "9.4.1",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.1",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/platform-express": "9.4.1",
    "@nestjs/swagger": "6.3.0",
    "@nestjs/terminus": "9.2.2",
    "@nestjs/throttler": "4.0.0",
    "@nodeteam/nestjs-pipes": "1.2.2",
    "@nodeteam/nestjs-prisma-pagination": "1.0.1",
    "@prisma/client": "^4.3.1",
    "@ucast/sql": "1.0.0-alpha.1",
    "adminjs": "^7.0.2",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express-basic-auth": "1.2.1",
    "module-alias": "2.2.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.2.0",
    "@nestjs/testing": "9.4.1",
    "@types/express": "4.17.13",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.5.0",
    "prettier": "2.3.2",
    "prisma": "4.3.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@filters": "./dist/filters",
    "@guards": "./dist/guards",
    "@pipes": "./dist/pipes",
    "@interceptors": "./dist/interceptors",
    "@constants": "./dist/constants",
    "@interfaces": "./dist/interfaces",
    "@decorators": "./dist/decorators",
    "@modules": "./dist/modules",
    "@config": "./dist/config",
    "@providers": "./dist/providers",
    "@helpers": "./dist/helpers",
    "@types": "./dist/types"
  }
}

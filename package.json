{
  "name": "nest-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate:up": "npx migrate-mongo up",
    "db:migrate:down": "npx migrate-mongo down"
  },
  "keywords": [
    "nestjs",
    "prisma",
    "typescript",
    "casl",
    "jwt",
    "starter",
    "boilerplate",
    "mongodb"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "3.354.0",
    "@aws-sdk/client-sqs": "3.354.0",
    "@casl/ability": "6.5.0",
    "@nestjs-plus/discovery": "2.0.2",
    "@nestjs/common": "10.0.2",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.2",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/platform-express": "10.0.2",
    "@nestjs/swagger": "7.0.2",
    "@nestjs/terminus": "10.0.1",
    "@nestjs/throttler": "4.1.0",
    "@nodeteam/nestjs-pipes": "1.2.5",
    "@nodeteam/nestjs-prisma-pagination": "1.0.6",
    "@prisma/client": "4.15.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.66",
    "@ucast/sql": "1.0.0-alpha.1",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "express-basic-auth": "1.2.1",
    "module-alias": "2.2.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sqs-consumer": "7.2.0",
    "sqs-producer": "3.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.3",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "migrate-mongo": "^10.0.0",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@filters": "./dist/filters",
    "@guards": "./dist/guards",
    "@pipes": "./dist/pipes",
    "@interceptors": "./dist/interceptors",
    "@constants": "./dist/constants",
    "@interfaces": "./dist/interfaces",
    "@decorators": "./dist/decorators",
    "@modules": "./dist/modules",
    "@config": "./dist/config",
    "@providers": "./dist/providers",
    "@helpers": "./dist/helpers",
    "@types": "./dist/types"
  }
}
